ated":{"line":127,"column":36},"source":"Details.js","original":{"line":74,"column":8}},{"generated":{"line":127,"column":37},"source":"Details.js","original":{"line":72,"column":6}},{"generated":{"line":127,"column":38},"source":"Details.js","original":{"line":71,"column":4}},{"generated":{"line":128,"column":0},"source":"Details.js","original":{"line":93,"column":3}},{"name":"React","generated":{"line":132,"column":2},"source":"Details.js","original":{"line":11,"column":22}},{"name":"Component","generated":{"line":132,"column":17},"source":"Details.js","original":{"line":11,"column":28}},{"name":"Details","generated":{"line":134,"column":15},"source":"Details.js","original":{"line":96,"column":15}}],"sources":{"Details.js":"import React from \"react\";\nimport pf from \"petfinder-client\";\nimport Carousel from \"./Carousel\";\nimport Modal from \"./Modal\";\n\nconst petfinder = pf({\n  key: process.env.API_KEY,\n  secret: process.env.API_SECRET\n});\n\nclass Details extends React.Component {\n  constructor(props) {\n    super(props);\n\n    this.state = {\n      loading: true,\n      showModal: true\n    };\n\n    this.toggleModel = this.toggleModel.bind(this);\n  }\n\n  toggleModel() {\n    this.setState({ showModal: !this.state.showModal });\n  }\n\n  componentDidMount() {\n    petfinder.pet\n      .get({\n        output: \"full\",\n        id: this.props.id\n      })\n\n      .then(data => {\n        const pet = data.petfinder.pet;\n        let breed;\n        if (Array.isArray(data.petfinder.pet.breeds.breed)) {\n          breed = pet.breeds.breed.join(\", \");\n        } else {\n          breed = pet.breeds.breed;\n        }\n\n        this.setState({\n          name: pet.name,\n          animal: pet.animal,\n          location: `${pet.contact.city}, ${pet.contact.state}`,\n          description: pet.description,\n          media: pet.media,\n          breed: breed,\n          loading: false\n        });\n      })\n      .catch(err => {\n        this.setState({ error: err });\n      });\n  }\n  render() {\n    if (this.state.loading) {\n      return <h1>loading</h1>;\n    }\n    const {\n      name,\n      animal,\n      breed,\n      location,\n      description,\n      media,\n      showModal\n    } = this.state;\n\n    return (\n      <div className=\"details\">\n        <Carousel media={media} />\n        <div>\n          <h1>{name}</h1>\n          <h2>\n            {animal} - {breed} - {location}\n          </h2>\n          <button onClick={this.toggleModel}> Adopt {name} </button>\n          <p>{description}</p>\n          {showModal ? (\n            <Modal>\n              <h1>Would you like to adopt {name}</h1>\n              <div className=\"buttons\">\n                <button onClick={this.toggleModel}> Yes </button>\n                <button onClick={this.toggleModel}> Definetly Yes </button>\n              </div>\n            </Modal>\n          ) : null}\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default Details;\n"},"lineCount":null}},"hash":"4fa83f7a5eece08c90ae066c352c96ea","cacheData":{"env":{"API_KEY":"1d5ea0e22d28536013d7aed881e3fefc","API_SECRET":"20ea7e9835dea3e2fd0421db7fa9fd7e"}}}